{
  "bracketSpacing": true,
  "singleQuote": true,
  "trailingComma": "all",
  "tabWidth": 2,
  "semi": false,
  "printWidth": 120,
  "jsxSingleQuote": true,
  "endOfLine": "auto",
  "singleAttributePerLine": false,
  "htmlWhitespaceSensitivity": "ignore",
  "bracketSameLine": false,
  "embeddedLanguageFormatting": "off",
  "plugins": ["prettier-plugin-tailwindcss"]
}
